# Production Environment Variables Example
# Copy this file to .env and fill in your actual values

# Django Settings
DJANGO_SETTINGS_MODULE=core.settings.production
DJANGO_SECRET_KEY=your-very-secret-key-here-change-this-in-production

# Database Configuration
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_HOST=localhost
DB_PORT=3306

# Allowed Hosts (comma-separated)
ALLOWED_HOSTS=goyard-demenagement.fr,www.goyard-demenagement.fr

# CORS Settings (comma-separated origins)
CORS_ALLOWED_ORIGINS=https://goyard-demenagement.fr,https://www.goyard-demenagement.fr

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Additional Settings (optional)
# DEBUG=False
# PYTHONPATH=/path/to/your/backend
